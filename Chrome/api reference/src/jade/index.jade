html(ng-app='app')
    head
        script(src='libs/angular.min.js')
        script(src='libs/angular-ui-router.min.js')
        script(src='libs/angular-animate.min.js')
        script(src='libs/angular-aria.min.js')
        script(src='libs/angular-material.min.js')
        script(src='libs/TweenMax.min.js')
        link(rel="stylesheet", href="libs/angular-material.min.css")
        link(rel="stylesheet", href="MainStyles.css")
        link(rel="stylesheet", href="assets/FA/css/font-awesome.min.css")
        script(src='js/setup.js') 
        script(src='js/templateCache.js')
        script(src='js/MODULES.js')
        script(src='js/APP.js')
    body(ng-controller='globalController as global')
        .global-container
            .global-inner
                header(ng-controller='headerController as header')
                    div(ui-view='header')
                main
                    div(ui-view='UI')
                footer
                    div(ui-view='footer')
